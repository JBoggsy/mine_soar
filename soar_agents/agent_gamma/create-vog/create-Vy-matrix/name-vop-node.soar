sp {create-Vy-matrix*propose*name-vop-node
   (state <s> ^name create-Vy-matrix
              ^top-state.svs.vsm.vog.node <node>)
   (<node> -^node-name)
   (<node>  ^node-id <node-id>
            ^op-name <> save-to-file)
-->
   (<s> ^operator <op> + =)
   (<op> ^name name-vop-node
         ^node-id <node-id>
         ^node-name |Vy matrix|)
}
