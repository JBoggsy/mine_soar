sp {create-vog*propose*create-get-vision
   (state <s> ^name create-vog
              ^top-state.svs.vsm.vog <vog>)
  -{(<vog> ^node <self-node>
           ^node <save-node>)
    (<self-node> ^node-name |vision|
                 ^node-id <self-node-id>)
    (<save-node> ^op-name save-to-file
                 ^target <self-node-id>)}
-->
   (<s> ^operator <op> + =)
   (<op> ^name create-get-vision)
}
